---
import { MarkdocRenderer } from 'astro-mdoc/src/virtual/imports.js'
import { getNamedImport } from 'astro-mdoc/src/utils/namedImports';
import { UniqueImports, MarkdocConfig, ProjectContext, UserConfig } from "astro-mdoc/src/virtual/imports.js";
const Renderer = await MarkdocRenderer({ source: `
    {% callout type="check" %}
        Markdoc is open-sourceâ€”check out its [source](http://github.com/markdoc/markdoc) to see how it works.
    {% /callout %}
` 
});
---
{
    Array.isArray(Renderer) ? (
        Renderer.map(Render => <Render />)
    ) : (
        <Renderer />
    )
}
<p>UniqueImports: {'['+ Object.keys(UniqueImports) + ']'}</p>
<p>MarkdocConfig: {JSON.stringify(MarkdocConfig)}</p>
<p>ProjectContext: {JSON.stringify(ProjectContext)}</p>
<p>UserConfig: {JSON.stringify(UserConfig)}</p>